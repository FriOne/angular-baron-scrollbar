{
  "name": "angular-baron-scrollbar",
  "version": "1.0.2",
  "description": "A wrapper for baron scrollbar library injecting required CSS automatically.",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -x angular -s angular-baron-scrollbar -t brfs -t browserify-shim -p browserify-derequire index.js > dist/index.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svbatalov/angular-baron-scrollbar.git"
  },
  "keywords": [
    "angular",
    "scroll",
    "baron",
    "browserify"
  ],
  "author": "Sergey Batalov",
  "license": "MIT",
  "devDependencies": {
    "brfs": "^1.4.1",
    "browserify": "^11.0.1",
    "browserify-derequire": "^0.9.4",
    "browserify-shim": "^3.8.10",
    "insert-css": "^0.2.0"
  },
  "dependencies": {
    "baron": "^0.7.10",
    "qwery": "^4.0.0",
    "xtend": "^4.0.0"
  },
  "browserify-shim": {
    "angular": "global:angular"
  }
}
